@inject IDialogService DialogService

<MudButton  Variant="Variant.Filled" OnClick="OpenDialogAsync" 
           Color="Color.Error" >
    <MudText Typo="Typo.body2">Cancel</MudText>
</MudButton>

@code {
    private Task OpenDialogAsync()
    {
        return DialogService.ShowAsync<CancelUploadDialog>("Cancel Upload Dialog");
    }
}